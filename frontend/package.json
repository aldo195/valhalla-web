{
  "name": "valhalla",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@antv/data-set": "^0.8.5",
    "antd": "^3.0.0",
    "axios": "^0.17.1",
    "bizcharts": "^3.1.2",
    "classnames": "^2.2.5",
    "emotion": "^8.0.10",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fittext": "^1.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "react-spinners": "^0.2.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "stylus": "^0.54.5",
    "stylus-chokidar": "^0.0.3"
  },
  "scripts": {
    "build-css": "stylus-chokidar src/",
    "watch-css": "npm run build-css && stylus-chokidar -w src/ --include-css",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-rewired build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,css,json}": [
      "prettier --write --no-bracket-spacing --single-quote --trailing-comma=all --print-width=120",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.4.1",
    "redux-devtools-extension": "^2.13.2"
  }
}
