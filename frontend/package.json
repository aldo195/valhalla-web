{
  "name": "valhalla",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@antv/data-set": "^0.8.6",
    "antd": "^3.4.0",
    "axios": "^0.18.0",
    "bizcharts": "^3.1.4",
    "classnames": "^2.2.5",
    "enquire.js": "^2.1.6",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "luxon": "^1.0.0",
    "path-to-regexp": "2.2.0",
    "rc-drawer-menu": "^0.5.7",
    "react": "^16.3.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.1",
    "react-fittext": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.14.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "stylus-chokidar src/",
    "watch-css": "npm run build-css && stylus-chokidar -w src/ --include-css",
    "start-js": "react-app-rewired start --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write --no-bracket-spacing --single-quote --trailing-comma=all --print-width=120",
      "git add"
    ]
  },
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.3",
    "@types/enquire.js": "^2.1.0",
    "@types/history": "^4.6.2",
    "@types/jsdom": "^11.0.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.105",
    "@types/luxon": "^0.5.0",
    "@types/node": "^9.6.2",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^16.3.5",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.5",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-thunk": "^2.1.0",
    "@types/stylus": "^0.48.29",
    "@types/typescript": "^2.0.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "husky": "^0.14.3",
    "jsdom": "^11.6.2",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.0",
    "redux-devtools-extension": "^2.13.2",
    "source-map-explorer": "^1.5.0",
    "stylus": "^0.54.5",
    "stylus-chokidar": "^0.0.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1"
  }
}
